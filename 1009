#include <stdio.h>

int main() {
    char nome[100];
    double salarioFixo, totalVendas, salarioTotal;

  
    scanf("%s", nome);
    scanf("%lf", &salarioFixo);
    scanf("%lf", &totalVendas);

   
    salarioTotal = salarioFixo + (totalVendas * 0.15);

    
    printf("TOTAL = R$ %.2lf\n", salarioTotal);

    return 0;
}
